{
  "name": "quick-connect",
  "version": "1.0.0",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.1",
    "crypto-js": "^3.1.9-1",
    "express": "^4.15.4",
    "jayson": "^2.0.4",
    "node-forge": "^0.7.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^4.8.4",
    "sequelize-cli": "^3.0.0-3",
    "sqlite3": "^3.1.11",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "build": "babel --presets es2015 src --out-dir lib --ignore test.js",
    "watch": "yarn build -- --watch",
    "start": "yarn build && node lib/server.js",
    "start-client": "yarn build && node lib/client.js",
    "test": "ava lib/test",
    "nuke": "yarn run sequelize db:drop; yarn run sequelize db:create; yarn run sequelize db:migrate; yarn run sequelize db:seed:all"
